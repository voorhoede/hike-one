{
  "version": 2,
  "name": "hike-one",
  "alias": [
    "now-v2.hike.one",
    "now-v2.www.hike.one",
    "now-v2.hikeone.com",
    "now-v2.www.hikeone.com",
    "now-v2.hikeone.nl",
    "now-v2.www.hikeone.nl"
  ],
  "builds": [{ "src": "server.js", "use": "@now/node-server", "config": { "maxLambdaSize": "50mb" } }],
  "env": {
    "DATO_API_TOKEN": "@hike_one_dato_api_token",
    "DOMAINS": "now-v2.hike.one now-v2.hikeone.com now-v2.hikeone.nl"
  },
  "regions": ["bru"],
  "github": {
    "autoAlias": false,
    "autoJobCancelation": true
  }
}
