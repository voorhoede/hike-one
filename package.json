{
  "name": "hike-one",
  "version": "1.0.0",
  "description": "website for Hike One",
  "private": "true",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "prebuild": "rimraf build/ && mkdir build/",
    "build": "npm-run-all --serial build:dato build:css build:next build:guide",
    "build:dato": "npm-run-all --serial build:dato:local build:dato:dump",
    "build:dato:staging": "ENVIRONMENT=staging npm run build:dato",
    "build:dato:local": "bash dato-dump.sh",
    "build:dato:dump": "node bin/dump-data",
    "build:next": "next build",
    "build:css": "node tasks/build-css.js",
    "build:guide": "npm-run-all --serial build:guide:stories copy:guide:*",
    "build:guide:stories": "build-storybook -c .storybook -o build/guide/",
    "deploy": "npm-run-all --serial deploy:site deploy:guide",
    "deploy:site": "npm-run-all --serial deploy:site:now deploy:site:alias",
    "deploy:site:now": "now -e DATO_API_TOKEN=@dato_api_token -e NODE_ENV=production -e TOKEN=@token -e DATO_URL=@dato_url",
    "deploy:site:alias": "now alias",
    "deploy:guide": "npm-run-all --serial deploy:guide:now deploy:guide:alias",
    "deploy:guide:now": "now deploy build/guide",
    "deploy:guide:alias": "cd build/guide/ && now alias",
    "dev": "npm-run-all --parallel watch:css serve:dev",
    "copy:guide:config": "ncp .storybook/now.json build/guide/now.json",
    "copy:guide:static": "ncp static/ build/guide/static/",
    "copy:guide:root": "ncp static/root/ build/guide/",
    "proxy": "ngrok http 3000",
    "serve:dev": "node server.js",
    "serve:guide": "start-storybook --port 3001 --config-dir .storybook --static-dir ./",
    "watch:css": "chokidar '**/*.less' --command 'npm run build:css'",
    "backupdata": "node tasks/backup-data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede/hike-one.git"
  },
  "author": "De Voorhoede",
  "license": "UNLICENSED",
  "dependencies": {
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.7",
    "datocms-client": "^0.5.4",
    "delay": "^3.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.15.3",
    "fontfaceobserver": "^2.0.13",
    "globby": "^8.0.1",
    "gsap": "^2.0.1",
    "helmet": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "next": "^6.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "request": "^2.81.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-stage-0": "7.0.0-beta.44",
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/addon-knobs": "^3.2.14",
    "@storybook/react": "^3.2.14",
    "chalk": "^2.1.0",
    "chokidar-cli": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.4.2",
    "gulp-less": "^3.3.2",
    "gulp-plumber": "^1.1.0",
    "gulp-pxtorem": "^3.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "ncp": "^2.0.0",
    "ngrok": "^3.0.1",
    "now": "^11.3.8",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.2"
  }
}
