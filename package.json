{
  "name": "hike-one",
  "version": "1.0.0",
  "description": "website for Hike One",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "prebuild": "rimraf build/ && mkdir build/",
    "build": "npm-run-all --serial backupdata build:dato build:css build:next",
    "build:dato": "dato dump --token=c6c5aac11814b76f9979",
    "build:next": "next build",
    "build:css": "node tasks/build-css.js",
    "build:guide": "build-storybook -c .storybook -o build/guide/",
    "dev": "npm-run-all --parallel watch:css serve:dev",
    "serve:dev": "node server.js",
    "serve:guide": "start-storybook -p 3001 -c .storybook -s ./",
    "watch:css": "chokidar '**/*.less' --command 'npm run build:css'",
    "backupdata": "node tasks/backup-data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede/hike-one.git"
  },
  "author": "De Voorhoede",
  "license": "All Rights Reserved",
  "private": true,
  "dependencies": {
    "express": "4.15.3",
    "next": "2.4.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "gsap": "1.20.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.1.9",
    "@storybook/addon-knobs": "3.1.9",
    "@storybook/react": "3.1.9",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chokidar-cli": "1.2.0",
    "datocms-client": "0.3.25",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-clean-css": "3.4.2",
    "gulp-less": "3.3.2",
    "gulp-plumber": "1.1.0",
    "gulp-pxtorem": "3.0.0",
    "gulp-sourcemaps": "2.6.0",
    "less": "2.7.2",
    "ncp": "2.0.0",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1"
  }
}
