{
  "name": "hike-one",
  "version": "2.13.1",
  "description": "website for Hike One",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "prebuild": "rimraf build/ && mkdir build/",
    "build": "npm-run-all --serial build:dato build:css build:next",
    "build:dato": "npm run build:dato:dump",
    "build:dato:staging": "ENVIRONMENT=staging npm run build:dato",
    "build:dato:dump": "node bin/dump-data",
    "build:next": "next build",
    "build:css": "node tasks/build-css.js",
    "deploy": "npm-run-all --serial deploy:site",
    "deploy:site": "npm-run-all --serial deploy:site:now deploy:site:alias",
    "deploy:site:now": "now -e DATO_API_TOKEN=@dato_api_token -e NODE_ENV=production -e TOKEN=@token -e DATO_URL=@dato_url",
    "deploy:site:alias": "now alias",
    "dev": "npm-run-all --parallel watch:css serve:dev",
    "proxy": "ngrok http 3000",
    "serve:dev": "nodemon server.js",
    "watch:css": "chokidar '**/*.less' --command 'npm run build:css'",
    "backupdata": "node tasks/backup-data",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint ./components/**/*.js ./pages/**/*.js",
    "lint:css": "stylelint './components/**/*.less' './styles/**/*.less' --syntax less"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede/hike-one.git"
  },
  "author": "De Voorhoede",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.18.0",
    "backoff": "^2.5.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.5.7",
    "datocms-client": "0.8.15",
    "delay": "^4.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "fontfaceobserver": "^2.1.0",
    "globby": "^9.2.0",
    "gsap": "^2.1.2",
    "helmet": "^3.18.0",
    "isomorphic-fetch": "^2.2.1",
    "next": "^8.1.0",
    "lodash": "^4.17.11",
    "prop-types": "15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.4.1",
    "request": "^2.88.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "10.0.1",
    "chalk": "^2.1.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-react": "7.13.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-pxtorem": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "ncp": "^2.0.0",
    "ngrok": "^3.0.1",
    "nodemon": "^1.19.0",
    "now": "^11.3.8",
    "npm-run-all": "^4.1.5",
    "stylelint": "10.0.1",
    "stylelint-config-standard": "18.3.0",
    "rimraf": "^2.6.3"
  }
}
